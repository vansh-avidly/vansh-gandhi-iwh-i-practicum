doctype html
html
    head
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body
        h1 Create a Record for Test Object
        form(action="/update-cobj" method="POST")
            label(for="object_id") Object ID:
            input(type="text" name="object_id" id="object_id" required)
            br
            label(for="name") Name:
            input(type="text" name="name" id="name" required)
            br
            label(for="bio") Bio:
            input(type="text" name="bio" id="bio" required)
            br
            button(type="submit") Submit
        br
        a(href="/") Return to the homepage